{
  "name": "trilobit-gmbh/contao-socialmedia-bundle",
  "type": "contao-bundle",
  "description": "Contao 4 / Contao 5 socialmedia bundle",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "trilobit GmbH",
      "homepage": "https://github.com/trilobit-gmbh"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "contao/core-bundle": "^4.13 || ^5.0",
    "codefog/contao-haste": "^4.0 || ^5.0"
  },
  "require-dev": {
    "contao/easy-coding-standard": "^3.0",
    "contao/manager-plugin": "^2.3",
    "friendsofphp/php-cs-fixer": "^v3.1.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "Trilobit\\SocialmediaBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Trilobit\\SocialmediaBundle\\ContaoManager\\Plugin"
  }
}
